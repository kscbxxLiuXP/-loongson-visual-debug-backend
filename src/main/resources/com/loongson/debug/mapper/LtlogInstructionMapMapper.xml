<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loongson.debug.mapper.LtlogInstructionMapMapper">
    <select id="getLtlogInstructionMapsComboed" resultType="com.loongson.debug.entity.LtlogInstructionMap" parameterType="java.lang.Integer">
        select operator, sum(num)as num from ltlog_instruction_map where ltid = #{ltid} group by operator order by num desc
    </select>
</mapper>
